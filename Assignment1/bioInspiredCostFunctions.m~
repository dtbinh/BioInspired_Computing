function [result, fx] = bioInspiredCostFunctions(functionName, range, dimensions)
    switch functionName
        case 'sphere'
            
            return 
        case 'quadric'
        case 'rastrigin'
        case 'rosenbrock'
            disp('zero')
        case 1
            disp('positive one')
        otherwise
            disp('other value')
    end

end